source 'https://rubygems.org'

gem "jekyll", "~> 4.4.1"

group :jekyll_plugins do
  gem "jekyll-remote-theme"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"         # Generates a sitemap.xml
  gem "jekyll-feed"            # Adds an RSS feed
  gem "jekyll-paginate"        # Enables pagination for blog posts
  gem "jekyll-timeago", "~> 0.14.0"  # Your custom plugin
end
